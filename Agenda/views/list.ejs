<h3>Consultas</h3>
<hr>

<form action="/searchresult" method="get">
    <input type="search" name="search" id="search" placeholder="cpf ou email">
    <button>Pesquisar</button>
</form>

 <%appos.forEach(appo => {%>
    <h1>Consulta: <%= appo.description %></h1>
    <hr>
    
    <h4>Paciente: <%= appo.name %></h4>
    <h4>Email: <%= appo.email %></h4>
    <h4>cpf : <%= appo.cpf %></h4>
    <hr>

<% })%>

